@model HandsonEmployee.Models.Employee
@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>
@using (Html.BeginForm())
{
<table>
    <tr>
        <td>Eid</td>
        <td>@Html.TextBoxFor(m => m.Eid)</td>
    </tr>
    <tr>
        <td>Name</td>
        <td>@Html.TextBoxFor(m => m.Name)</td>
    </tr>
    <tr>
        <td>designation</td>
        <td>@Html.DropDownListFor(m => m.Designation, ViewBag.Designation as SelectList)</td>
    </tr>
    <tr>
        <td>ProjectName</td>
        <td>@Html.DropDownListFor(m => m.ProjectName, ViewBag.ProjectName as SelectList)</td>
    </tr>
    <tr>
        <td>Password</td>
        <td>@Html.PasswordFor(m=>m.Pwd)</td>
    </tr>
    <tr>
        <td colspan="2">
            <input type="submit" value="Register" />
        </td>
    </tr>
</table>
}
